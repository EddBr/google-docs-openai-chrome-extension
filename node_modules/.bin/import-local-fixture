#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/import-local/fixtures/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/import-local/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules:/home/eddie/Code/chrome_extensions/node_modules:/home/eddie/Code/node_modules:/home/eddie/node_modules:/home/node_modules:/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/import-local/fixtures/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/import-local/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules:/home/eddie/Code/chrome_extensions/node_modules:/home/eddie/Code/node_modules:/home/eddie/node_modules:/home/node_modules:/node_modules:/home/eddie/Code/chrome_extensions/Hyperlink/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
